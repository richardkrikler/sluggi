<?xml version="1.0"?>
<psalm resolveFromConfigFile="true"
       cacheDirectory="../.Build/"
       errorLevel="5"
       hideExternalErrors="true"
       phpVersion="8.2">
    <projectFiles>
		<directory name="../Classes" />
		<ignoreFiles>
			<directory name="../Build" />
			<directory name="../.Build" />
		</ignoreFiles>
    </projectFiles>
	<issueHandlers>
		<UnusedClass>
			<errorLevel type="suppress">
				<directory name="../Classes"/>
			</errorLevel>
		</UnusedClass>
		<UnusedParam>
			<errorLevel type="suppress">
				<directory name="../Classes/DataHandler"/>
			</errorLevel>
		</UnusedParam>
	</issueHandlers>
</psalm>
